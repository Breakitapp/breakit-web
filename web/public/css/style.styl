/*breaks*/
body
	margin:0
	padding:0

#breakFeed
	overflow: scroll

#BreakHeaderHolder
	width:95%
	min-height:90px
	margin-left:auto
	margin-right:auto

#BreakHeaderLeft
	width: 40%
	float: left
	p
		margin-top:-2px
		display:inline
	img
		width:40px
	h2
		margin-top:10px
#BreakHeaderRight
	width: 60%
	text-align:right
	float:right
	ul
		margin-top:-10px
#BreakHeaderHolder, #breakContent, form
	margin-left:auto
	margin-right:auto
#BreakContent
	width:99%
	img
		margin-top:-20px
#BreakFooter
	width:100%
	p
		margin-top:0px
		line-height:18px
	ul
		margin-left:-40px
		margin-top:-10px
	li
		padding-right:10px
#DivComments
	position:absolute
	z-index:1
	font-size:20px
	margin-top:-70px
	margin-left:90px
#DivViews
	position:absolute
	z-index:10
	font-size:15px
	margin-top:-50px
	margin-left:150px
	color:white
.PlaceDots
	float:right
	margin-top:-20px
.red
	color:red
p
	line-height:8px
ul
	list-style:none
li
	display:inline
#DivLi
	max-height:20px
	overflow:hidden

/*usertemplate*/
label
	float:left
	width:90px
form
	margin-top:30px
	margin-bottom:-20px
	border-style:outset
	border-color:grey
	width:90%
	background-color:#646464
	color:#EDE8EA

	
/*Web view (for media interface) */

#web_interface
	width:100%

.top_break
	float:left
	margin: 10px